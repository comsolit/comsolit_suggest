
plugin.tx_comsolitsuggest {
	view {
		# cat=plugin.tx_comsolitsuggest/file; type=string; label=Path to template root (FE)
		templateRootPath = EXT:comsolit_suggest/Resources/Private/Templates/
		# cat=plugin.tx_comsolitsuggest/file; type=string; label=Path to template partials (FE)
		partialRootPath = EXT:comsolit_suggest/Resources/Private/Partials/
		# cat=plugin.tx_comsolitsuggest/file; type=string; label=Path to template layouts (FE)
		layoutRootPath = EXT:comsolit_suggest/Resources/Private/Layouts/
	}
	persistence {
		# cat=plugin.tx_comsolitsuggest//a; type=string; label=Default storage PID
		storagePid =
	}
	settings {
		# cat=plugin.tx_comsolitsuggest/file; type=int+; label=Limit suggest words (val 5 to 20)
		limit = 10
		# cat=plugin.tx_comsolitsuggest/file; type=int+; label=Minimum word length for suggest (val 1 to 10)
		minLength = 1
		# cat=plugin.tx_comsolitsuggest/file; type=boolean; label=Extra include jquery (1 or 0)
		includeJquery = 0
		# cat=plugin.tx_comsolitsuggest/file; type=string; label=Placeholder in search input (text)
		placeholder = suchen
		# cat=plugin.tx_comsolitsuggest/file; type=int+; label=Page ID where the Indexedsearch Plugin is (PID)
		searchPid = 40
		# cat=plugin.tx_comsolitsuggest/file; type=string; label=Required when TYPO3 root is placed in subdirectory
		URL =
	}
}
